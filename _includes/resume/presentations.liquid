<ul class="list-unstyled">
  {% for item in data[1] %}
    <li class="mb-2">
      <strong>{{ item.name }}</strong><br/>
      {% if item.publisher %}<em>{{ item.publisher }}</em>{% endif %}
      {% if item.location %} â€” {{ item.location }}{% endif %}
      {% if item.releaseDate %} ({{ item.releaseDate }}){% endif %}
      <br/>
      {{ item.summary }}
      {% if item.url %}
        <br/><a href="{{ item.url }}" target="_blank">{{ item.url }}</a>
      {% endif %}
    </li>
  {% endfor %}
</ul>
